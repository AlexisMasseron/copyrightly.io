{
  "name": "copyrightly.eth",
  "version": "0.0.1",
  "description": "√êApp for Copyright Management",
  "scripts": {
    "postinstall": "node patch.js",
    "lint": "npm-run-all -p -c solhint nglint",
    "solhint": "solhint ./contracts/*.sol",
    "nglint": "ng lint",
    "network": "ganache-cli -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat'",
    "migrate": "truffle migrate --reset",
    "test": "npm-run-all -p -c soltest ngtest",
    "soltest": "truffle test",
    "ngtest": "ng test --watch=false",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-heroku": "ng build --prod -e heroku",
    "e2e": "ng e2e",
    "e2e-local": "npm-run-all e2e-quick e2e-report --continue-on-error",
    "e2e-quick": "ng e2e --dev-server-target= --webdriver-update false",
    "e2e-report": "node e2e/generate-html-report.js",
    "webdriver-update": "webdriver-manager update",
    "debug:migrate": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle migrate --reset",
    "debug:soltest": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/pwa": "^0.7.0",
    "@angular/router": "^6.1.0",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^2.2.1",
    "bootstrap-css-only": "^4.1.1",
    "core-js": "^2.5.4",
    "ipfs-api": "^23.0.0",
    "rxjs": "^6.0.0",
    "web3": "^1.0.0-beta.35",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.1.2",
    "codelyzer": "~4.2.1",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.3",
    "ganache-cli": "^6.1.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.0.0",
    "solhint": "^1.2.1",
    "solidity-cli": "^1.0.1",
    "truffle": "^4.1.13",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zos-lib": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogargon/copyrightly.eth.git"
